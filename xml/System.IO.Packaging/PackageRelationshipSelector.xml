    <altmember cref="T:System.IO.Packaging.PackageRelationship" />
    <altmember cref="T:System.IO.Packaging.PackagePart" />
    <related type="ExternalDocumentation" href="http://go.microsoft.com/fwlink/?LinkId=74494">Digital Signing Framework der Open Packaging Conventions</related>
